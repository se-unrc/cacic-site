             <div class="overlay-box">    
               <div class="display-table">    
                 <div class="display-table-cell">   
                 </div><!-- /.display-table-cell -->    
               </div><!-- /.display-table -->   
             </div><!-- /.overlay-box -->   
           </div><!-- /.img-box -->   
           <h3><%= link_to 'Fernanda Carmona', target: "_blank" %></h3>    
