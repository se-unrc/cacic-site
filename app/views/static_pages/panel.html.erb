<section class="speaker-section sec-pad speaker-page">
  <div class="container">
    <div class="sec-title text-center">
      <h5>CACIC 2019</h5>
      <h2><%= t 'plenary_activities' %></h2>
      <h3><u> Panel</u></h3>
      <h3>“El impacto de la investigación en la docencia”</h3>
      <div class="register-section">
            <a href="https://forms.gle/UVuEnGBcQQFtaNAK6" target="_blank" class="btn btn-default register-button" role="button">
            <%= t 'registration' %>
          </a>
      </div> 
      <h5> <%= fa_icon "calendar" %>  <%= t 'date3' %>. 20hs </h5> 


    </div><!-- /.sec-title text-center -->
    
    <div class="row">    
       <div class="col-md-3">   
         <div class="single-speaker">   
