               <div class="display-table">    
               </div><!-- /.display-table -->   
             </div><!-- /.overlay-box -->   
           </div><!-- /.img-box -->   
           <h3><%= link_to 'Marcelo Frias', 'https://www.conicet.gov.ar/new_scp/detalle.php?id=21730&datos_academicos=yes', target: "_blank" %></h3>
            <p><%= t 'uni_itba' %></p>
          </div><!-- /.single-speaker -->    
       </div><!-- /.col-md-3 -->    

        <div class="col-md-3">    
